<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MobileStepElement">
        <attr name="stepTitle" format="string" />
        <attr name="stepIcon" format="enum">
            <enum name="active" value="0" />
            <enum name="inactive" value="1" />
            <enum name="edit" value="3" />
            <enum name="error" value="4" />
            <enum name="checked" value="5" />
        </attr>
    </declare-styleable>
</resources>