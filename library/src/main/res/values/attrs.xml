<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StepElement">
        <attr name="stepTitle" format="string" />
        <attr name="stepIcon" format="enum">
            <enum name="active" value="0" />
            <enum name="inactive" value="1" />
            <enum name="edit" value="2" />
            <enum name="error" value="3" />
            <enum name="checked" value="4" />
            <enum name="errorActive" value="5" />
        </attr>
        <attr name="stepSubtext" format="string" />
        <attr name="stepNumber" format="string" />
        <attr name="stepOptional" format="boolean" />
    </declare-styleable>
</resources>